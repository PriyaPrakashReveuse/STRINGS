#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    string s="bac.ant";
    int count=0;
    
    for(int end=0;end<S.length();end++)
    {
            if(S[end]=='.')
            {
                reverse(S.begin()+count,S.begin()+end);
                count=end+1;
            }
    }
    
    reverse(S.begin()+count,S.end());
    reverse(S.begin(),S.end());
    return S;
    
}
